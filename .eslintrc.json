{
  "extends": "./build-tools/node_modules/eslint-config-vaadin/index.js",
  "rules": {
    "linebreak-style": 0,
    "object-curly-spacing": 0

  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "Polymer": false,
    "Vaadin": false
  },
  "parserOptions": {
      "sourceType": "module"
  }
}
